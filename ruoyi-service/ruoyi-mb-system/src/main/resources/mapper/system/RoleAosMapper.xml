<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.RoleAosMapper">

    <insert id="insertRoleAos" parameterType="com.ruoyi.system.domain.RoleAos">
        insert into role_aos (
        <if test="roleId != null and roleId != '' ">role_id,</if>
        <if test="aosNo != null and aosNo != '' ">aos_no</if>
        )values(
        <if test="roleId != null and roleId != ''">#{roleId},</if>
        <if test="aosNo != null and aosNo != ''">#{aosNo}</if>
        )
    </insert>

    <delete id="deleteByIds" parameterType="Long">
        delete from role_aos where role_id in
        <foreach collection="array" item="roleId" open="(" separator="," close=")">
            #{roleId}
        </foreach>
    </delete>

</mapper>
