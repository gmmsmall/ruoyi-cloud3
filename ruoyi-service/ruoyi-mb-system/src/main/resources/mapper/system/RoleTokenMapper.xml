<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.RoleTokenMapper">

    <insert id="insertRoleToken" parameterType="com.ruoyi.system.domain.RoleToken">
        insert into role_token (
        <if test="roleId != null and roleId != '' ">role_id,</if>
        <if test="tokenNo != null and tokenNo != '' ">token_no</if>
        )values(
        <if test="roleId != null and roleId != ''">#{roleId},</if>
        <if test="tokenNo != null and tokenNo != ''">#{tokenNo}</if>
        )
    </insert>

    <delete id="deleteByIds" parameterType="Long">
        delete from role_token where role_id in
        <foreach collection="array" item="roleId" open="(" separator="," close=")">
            #{roleId}
        </foreach>
    </delete>

</mapper>
